{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "biome check .",
		"format": "biome check --write --unsafe .",
		"lint:ci": "biome ci .",
		"format:write": "biome format --write .",
		"lint:apply": "biome lint --apply .",
		"check:apply": "biome check --apply .",
		"check:apply-unsafe": "biome check --write --unsafe .",
		"postcss:build": "postcss src/app.css -o tmp/app.css",
		"openapi:fetch": "mkdir -p src/lib/api && curl -H \"Accept: application/json\" http://localhost:8000/openapi.json -o src/lib/api/openapi.json",
		"openapi:generate": "bunx orval"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/postcss": "^4.1.5",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.1.5",
		"@tanstack/svelte-query": "^5.75.0",
		"@types/leaflet": "^1.9.18",
		"autoprefixer": "^10.4.21",
		"daisyui": "^5.0.35",
		"globals": "^16.0.0",
		"orval": "^7.9.0",
		"postcss": "^8.5.3",
		"stylelint": "^16.19.1",
		"stylelint-config-standard": "^38.0.0",
		"stylelint-config-tailwindcss": "^1.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.8.3",
		"vite": "^6.2.6",
		"zod": "^3.24.3"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.49.9",
		"axios": "^1.9.0"
	}
}
